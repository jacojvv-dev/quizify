{
    "ignore": [
        "node_modules/**"
    ],
    "env": {
        "test": {
            "presets": [
                "es2015"
            ],
            "plugins": [
                "transform-object-assign",
                "transform-object-rest-spread"
            ]
        },
        "release": {
            "presets": [
                [
                    "es2015",
                    {
                        "modules": false
                    }
                ]
            ],
            "sourceMap": false,
            "plugins": [
                "transform-object-assign",
                "transform-object-rest-spread"
            ]
        }
    }
}